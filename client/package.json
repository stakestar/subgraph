{
  "name": "@stakestar/subgraph-client",
  "description": "GraphQL client for consuming StakeStar's The Graph GraphQL API",
  "version": "0.1.4",
  "author": "StakeStar",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stakestar/subgraph.git"
  },
  "license": "UNLICENSED",
  "source": "src/.graphclient/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "@stakestar:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "start": "parcel watch",
    "lint": "eslint -c .eslintrc.js --ext .js,.ts .",
    "bump": "bump --commit 'Release subgraph-client v' --tag 'subgraph-client-v'",
    "build:graphclient": "cd ./src && sh build.sh",
    "build:package": "parcel build"
  },
  "devDependencies": {
    "@graphprotocol/client-cli": "^2.2.9",
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "@parcel/packager-ts": "2.7.0",
    "@parcel/transformer-typescript-types": "2.7.0",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "8.22.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-promise": "^6.0.1",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "prettier-plugin-sorted": "^2.0.0",
    "typescript": "4.7.4"
  },
  "importSort": {
    ".js, .ts": {
      "options": {
        "cacheStrategy": "directory",
        "wildcardAtStart": false,
        "ignoredAliases": [],
        "bottomAliases": []
      }
    }
  },
  "dependencies": {
    "@graphql-mesh/cache-localforage": "^0.6.39",
    "@graphql-mesh/cross-helpers": "^0.2.6",
    "@graphql-mesh/graphql": "^0.31.10",
    "@graphql-mesh/http": "^0.1.7",
    "@graphql-mesh/merger-bare": "^0.15.23",
    "@graphql-mesh/runtime": "^0.44.4",
    "@graphql-mesh/store": "^0.8.42",
    "@graphql-mesh/types": "^0.83.4",
    "@graphql-mesh/utils": "^0.41.9",
    "@whatwg-node/fetch": "^0.4.3",
    "graphql": "^16.6.0"
  }
}
